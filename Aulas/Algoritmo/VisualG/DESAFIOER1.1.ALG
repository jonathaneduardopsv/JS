Var
   C:Inteiro


Inicio
   escreval ("=====================")
   escreval ("")
   escreval ("CONTAGEM INTELIGENTE")
   escreval ("")
   escreval ("=====================")
   escreval ("")
   escreval ("Inicio : 6")
   escreval ("Termino : 0")
   escreval ("")
   escreval ("=====================")
   escreval ("")
   escreval ("       CONTANDO      ")
   escreval ("")
   escreval ("=====================")
   escreval ("")
   C <-6
   enquanto (C >= 0) faca
            escreva (C, "..")
