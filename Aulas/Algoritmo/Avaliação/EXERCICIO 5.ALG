Algoritmo "Exercicio 5"
//
//
Var
   n, ant, suc:inteiro

Inicio

   escreva("Digite um número: ")

   leia (n)

   escreval

   ant<- n -1

   suc<- n +1

   escreval("O antecessor de ", n, " é", ant)
   
   escreval

   escreval("O sucessor de ", n, " é", suc)


Fimalgoritmo