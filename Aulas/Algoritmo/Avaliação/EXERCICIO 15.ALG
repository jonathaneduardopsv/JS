Algoritmo "Exercicio 15"
//
//
Var

   C, N, R:inteiro
   
Inicio

   C <- 1
   
   Escreva ("Qual Número quer multiplicar ? ")
   
   Leia (N)
   
   Repita
   
      R <- N * C
      
      Escreval (N,"x",C,"=",R)
      
      C <- C + 1
      
   Ate (C > 10)

Fimalgoritmo