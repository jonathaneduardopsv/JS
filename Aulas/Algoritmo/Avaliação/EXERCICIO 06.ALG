Algoritmo "Exercicio 06"
//
//

Var

   km, pagar:real

   dias:inteiro

Inicio

   escreva("Quantos Km foram percorridos? ")

   leia(km)

   escreval

   escreva("Quantos dias o carro permaneceu alugado? ")

   leia(dias)

   escreval

   pagar <- km * 0.2 + dias * 90

   escreva("O valor total a cobrar é: ",pagar)


Fimalgoritmo