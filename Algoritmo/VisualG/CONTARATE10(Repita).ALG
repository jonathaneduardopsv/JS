Algoritmo "ContarAte10"
//
// Descrição   : ESTRUTURAS DE REPETIÇÃO
// Autor(a)    : Jonathan
// Data atual  : 25/9/2022
Var
   C, N, R:inteiro
Inicio
   C <- 1
   Escreva ("Qual Número quer multiplicar ? ")
   Leia (N)
   repita
      R <- N * C
      escreval (N,"x",C,"=",R)
      C <- C + 1
   ate (C > 10)



Fimalgoritmo